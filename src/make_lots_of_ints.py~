#!/usr/bin/env/python
import random


def print_int_line(min_, max_, num_ints):
    print(*(random.randint(min_, max_) for i in range(num_ints)))


def main():
    random.seed(1)
    for line in range(5):
        print_int_line(min_=0, max_=1000, num_ints=random.randint(1000, 50000))


if __name__ == '__main__':
    main()
